    "@babel/preset-env": "^7.0.0-beta.42",
    "babel-cli": "^6.26.0",
    "babel-core": "~6.26.0",
    "babel-loader": "~7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "expect": "^22.4.3",
